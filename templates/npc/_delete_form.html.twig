<form class="delete-form" method="post" action="{{ path('npc_delete', {'id': npc.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ npc.id) }}">
    <button class="btn custom-color-orange">Delete</button>
</form>
